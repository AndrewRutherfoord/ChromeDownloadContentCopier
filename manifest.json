{
  "name": "Download Content Copier",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "permissions": ["downloads", "clipboardWrite", "activeTab", "storage"],
  "action": {
    "default_popup": "popup/popup.html"
  }
}
